    <validationRules>
        <fullName>PF_Start_End_date_check</fullName>
        <active>true</active>
        <description>Start date cannot be greater than End date</description>
        <errorConditionFormula>PF_Start_Date__c &gt; PF_End_Date__c</errorConditionFormula>
        <errorMessage>Start date cannot be greater than End date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StoryStatusReason</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL( PF_Story_Status__c , &apos;Out of Scope&apos;) ,ISPICKVAL( PF_Story_Status__c , &apos;Duplicate&apos;),ISPICKVAL( PF_Story_Status__c , &apos;Cancelled&apos;)) ,  ISBLANK( PF_Story_Status_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>PF_Story_Status_Reason__c</errorDisplayField>
        <errorMessage>Please enter Story Status Reason when Story Status is Out of Scope or Duplicate or Cancelled</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Story_dates_lies_between_Sprint</fullName>
        <active>false</active>
        <description>Story Start/End dates should lie within Sprint Start/end dates</description>
        <errorConditionFormula>(PF_Start_Date__c &lt; PF_Sprint__r.PF_Start_Date__c ) || 
(PF_End_Date__c &gt; PF_Sprint__r.PF_End_Date__c )</errorConditionFormula>
        <errorMessage>Story Start/End dates should lie within Sprint Start/end dates</errorMessage>
    </validationRules>
