    <validationRules>
        <fullName>CRT_Check</fullName>
        <active>true</active>
        <errorConditionFormula>CRT__c = TRUE

&amp;&amp; 

ISBLANK( Distribution_List__c )

&amp;&amp;

(NOT(ISBLANK(Contact__c)))

&amp;&amp; 

ISBLANK (Contact__r.Email)</errorConditionFormula>
        <errorDisplayField>CRT__c</errorDisplayField>
        <errorMessage>If CRT is checked, then Contact should have an email address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Role_required_for_Contact</fullName>
        <active>true</active>
        <description>This rule will ensure that user enters the contact role if Contact is selected.</description>
        <errorConditionFormula>AND( AND(ISBLANK( Contact__c ),ISBLANK(Distribution_List__c)), NOT(ISBLANK(TEXT( Contact_Role__c ))))
|| 
AND( OR(NOT(ISBLANK( Contact__c )),NOT(ISBLANK(Distribution_List__c))), ISBLANK(TEXT( Contact_Role__c )))</errorConditionFormula>
        <errorMessage>Please enter both Contact/DL and Contact Role</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ForceSelectionofEitherDLorContact</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Contact__c  &lt;&gt; NULL,  Distribution_List__c &lt;&gt; NULL)</errorConditionFormula>
        <errorMessage>Please enter a contact or a DL, but not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Org_role_is_required_for_org_association</fullName>
        <active>true</active>
        <description>This rule will ensure that user enters the org role if org is selected.</description>
        <errorConditionFormula>AND(ISBLANK(Contact__c),ISBLANK(Distribution_List__c), NOT(ISBLANK(Organization1__c)), ISBLANK(Role__c))</errorConditionFormula>
        <errorMessage>Please enter Organization Role</errorMessage>
    </validationRules>
