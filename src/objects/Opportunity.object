    <validationRules>
        <fullName>Close_Reason_Required</fullName>
        <active>true</active>
        <description>Closed Reason required for lost opportunity</description>
        <errorConditionFormula>( ISPICKVAL(StageName , &apos;Closed Lost&apos;) )
&amp;&amp;
(ISPICKVAL( Closed_Reason__c ,&apos;&apos;) )</errorConditionFormula>
        <errorDisplayField>Closed_Reason__c</errorDisplayField>
        <errorMessage>Please provide close reason for lost opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ConsultantNameIsRequired</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( Is_Consultant__c ,&apos;Yes&apos;),ISBLANK( Consultant__c ) )</errorConditionFormula>
        <errorDisplayField>Consultant__c</errorDisplayField>
        <errorMessage>Consultant Name is required for this opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PreventOppLineItemDeletion</fullName>
        <active>true</active>
        <errorConditionFormula>No_Of_Known_Item__c &lt;  PRIORVALUE(No_Of_Known_Item__c)</errorConditionFormula>
        <errorMessage>Only Unknown Strategies are allowed to delete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Edit_on_Closed_records</fullName>
        <active>true</active>
        <description>This prevents edit on closed records</description>
        <errorConditionFormula>PRIORVALUE(isClosed__c) = TRUE</errorConditionFormula>
        <errorMessage>This Opportunity record has been closed and may not be edited.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_is_required_bfr_stage_is_Funded</fullName>
        <active>false</active>
        <errorConditionFormula>No_Of_Known_Item__c = 0 &amp;&amp; OR((TEXT(StageName) = &apos;Funded&apos;), (TEXT(StageName) = &apos;Semi-Finals&apos;), (TEXT(StageName) = &apos;Finals&apos;), (TEXT(StageName) = &apos;Contracting&apos;), (TEXT(StageName) = &apos;Pending Flow&apos;), (TEXT(StageName) = &apos;Funded&apos;), (TEXT(StageName) = &apos;Investment Meeting&apos;), (TEXT(StageName) = &apos;Finals/Pricing&apos;), (TEXT(StageName) = &apos;Closed Won&apos;))&amp;&amp;  Original_Opportunity__c = NULL</errorConditionFormula>
        <errorMessage>At least one Strategy is required before advancing the stage passed &quot;Qualified&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RejectedProductOpp</fullName>
        <active>true</active>
        <description>Opportunity should not allowed to move if any of the custom product is rejected</description>
        <errorConditionFormula>AND( Num_Rejected_Products__c &gt;0, ISCHANGED( StageName ))</errorConditionFormula>
        <errorMessage>Opportunity stage cannot be changed due to one or more rejected products.</errorMessage>
    </validationRules>
