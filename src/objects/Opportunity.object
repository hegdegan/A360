        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot; )}
var oppRecordTypeId = &apos;{!$Label.OPP_RT_INS}&apos;;
var oppFieldIds = sforce.connection.query(&quot;SELECT Name, Field_API_Name__c, Field_Id__c, Field_Label__c, Object_Name__c from StoreFieldIds__c where Object_Name__c = &apos;Opportunity&apos; &quot;);

var recordTypeQuery = sforce.connection.query(&quot;SELECT Name,Id from RecordType WHERE developername = &apos;Global_Institutional_Client&apos; and SobjectType = &apos;Account&apos; &quot;);
var recordTypeQueryResult = recordTypeQuery.getArray(&quot;records&quot;);
var RecordTypeIdForAcc = recordTypeQueryResult[0].Id;


var accRecFieldIds = sforce.connection.query(&quot;Select id,Name,Distribution_Sub_Channel__c,RecordTypeId from Account where Id = &apos;{!Account.Id}&apos; &quot;);
var accrecords = accRecFieldIds.getArray(&quot;records&quot;);
var accRecTypeId = accrecords[0].RecordTypeId;

var accountF;
var accountFLKId;
var nameF;
var regionF;
var isConsF;
var RecordType = &apos;RecordType&apos;;
records = oppFieldIds.getArray(&quot;records&quot;);

for (var i=0; i&lt; records.length; i++) {
var record = records[i];
if(record.Field_API_Name__c == &apos;Account&apos;){
accountF = record.Field_Id__c;
accountFLKId = accountF + &apos;_lkid&apos;;
}else if(record.Field_API_Name__c == &apos;Name&apos;){
nameF = record.Field_Id__c;
}else if(record.Field_API_Name__c == &apos;Region&apos;){
regionF = record.Field_Id__c;
}else if(record.Field_API_Name__c == &apos;Is_Consultant__c&apos;){
isConsF = record.Field_Id__c;
}

}
var oppURL;
if(accRecTypeId == RecordTypeIdForAcc){
oppURL = &apos;/006/e?retURL={!Account.Id}&amp;&apos;+nameF+&apos;={!Account.Name}&amp;&apos;+accountF+&apos;={!Account.Name}&amp;&apos;+accountFLKId+&apos;={!Account.Id}&amp;&apos;+regionF+&apos;={!Account.Distribution_Sub_Channel__c}&amp;RecordType=&apos; + oppRecordTypeId +&apos;&amp;&apos;+isConsF+&apos;=No&apos;
}else {
oppURL = &apos;/006/e?retURL={!Account.Id}&amp;&apos;+nameF+&apos;={!Account.Name}&amp;&apos;+accountF+&apos;={!Account.Name}&amp;&apos;+accountFLKId+&apos;={!Account.Id}&amp;&apos;+regionF+&apos;={!Account.Distribution_Sub_Channel__c}&amp;RecordType=&apos; + oppRecordTypeId
}

if (typeof(srcUp) == &apos;function&apos;) {
srcUp(oppURL);
} else {
window.open(oppURL);
}</url>
