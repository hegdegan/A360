{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/22.0/apex.js&quot; )} 

//parameters to pass in the URL 
var returnID; 
var pagename; 
var isTaskorEvent = &apos;task=true&apos;; 
var isNew = &apos;isNew=true&apos;;
var RecType;  

//Determine the Page Layout (Institutional/Retail) 
if(&apos;{!Business_at_Risk__c.Type__c}&apos;.includes(&quot;Institution&quot;) || &apos;{!Business_at_Risk__c.Type__c}&apos;.includes(&quot;Global Client Service&quot;) || &apos;{!Business_at_Risk__c.Type__c}&apos;.includes(&quot;Global Consultant Relations&quot;)){ 
pagename = &apos;MFS_Activity360PageSF1&apos;;
RecType = &apos;Institutional&apos;;  
} else 
{ 
if(&apos;{!Business_at_Risk__c.Type__c}&apos;.includes(&quot;Retail&quot;)){ 
pagename = &apos;MFS_Activity360RetailPageSF1&apos;; 
RecType = &apos;Retail&apos;;
}else{ 
pagename = &apos;MFS_Activity360RetailPageSF1&apos;; 
RecType = &apos;Retail&apos;;
} 
} 
//Determine the return ID (Account/Contact/Oppty/BAR id) 
var barid = &apos;{!Business_at_Risk__c.Id}&apos;; 
returnID = barid; 

var activityId; 

var connection = sforce.connection; 
var newActivityDetail= new sforce.SObject(&quot;Activity_Detail__c&quot;); 
newActivityDetail.Subject__c = &apos;Orphan Record&apos;; 
