//alert(result); 

activityId = result[0].id; 

var A360url = &apos;/apex/&apos; + pagename + &apos;?Id=&apos;+activityId +&apos;&amp;RecType=&apos; + RecType + &apos;&amp;&apos;+isTaskorEvent+&apos;&amp;returnID=&apos;+returnID +&apos;&amp;&apos;+ isNew + &apos;&amp;isConsole=false&apos;; 

