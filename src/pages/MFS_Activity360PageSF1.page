    var hasPageModifiedInConsole = false;
       
    $(document).ready(function(){
        var currWhatID = $.trim($(".what_id_object_name_new").text());
        var currWhoID = $.trim($(".what_name_new").text());
        
        populateRelatedToField(currWhatID, currWhoID);
    });
    
    function SetTabTitle() {
        //Set the current tab's title
        if('{!eventCallNote}'=='true')
            if('{!callNote.Not_An_Orphan_Record__c}'=='false')
                sforce.console.setTabTitle('{!$Label.A360_New_Activity}');
            else
                sforce.console.setTabTitle('{!parentEvent.Subject}');
        else
            if('{!callNote.Not_An_Orphan_Record__c}'=='false')
                sforce.console.setTabTitle('{!$Label.A360_New_Call}');
            else
                sforce.console.setTabTitle('{!parentTask.Subject}');
    }
    var pageLoad = window.onload;
    window.onload = function() {
        if (pageLoad) {
            pageLoad();
        }
        SetTabTitle();
    }
    //JS function to support reRender of rich text field
    function ReloadCKEditor() {
        for(name in CKEDITOR.instances)
        {
            delete CKEDITOR.instances[name];
        }
        CKEDITOR.replaceAll();
    } 
    
    function createSpeakerRequest1(isConsole, activityDetailId, startDate, endDate, parentWhoId, parentSubject, parentWhatId, assignedTo, UserId,UserName, parentWhoName, parentWhoType, parentWhatName, parentWhatType ){
        var url = '/'+'{!erObjectId}'+'/e?retURL=%2Fa30%2Fo&RecordType='+'{!erRecordTypeId}&'+activityDetailId+
            '&'+'{!erContactId}'+'_lkid='+parentWhoId+'&'+'{!erContactId}'+'='+parentWhoName+'&'+'{!erStDateId}'+'='+startDate+'&'+'{!erEndDateId}'+'='+endDate+'&'+'{!eventTitleId}'+'='+parentSubject+
            '&'+'{!erOrgId}'+'_lkid='+parentWhatId+'&'+'{!erOrgId}'+'='+parentWhatName+'&'+'{!requestorId}'+'_lkid='+UserId+'&'+'{!requestorId}'+'='+ UserName;
        
        if(sforce.console.isInConsole() || isConsole == 'true'){
            OpenSpkrRequestPrimaryTab(url);
        }
        else if((typeof sforce != 'undefined') && sforce && (!!sforce.one)){
            // Salesforce1 navigation
            sforce.one.createRecord('Event_Request__c','{!erRecordTypeId}');
        }
        else{
            window.open(url);   
        } 
    }
    
    function openMeetingPrepSheet(isConsole){
        if(sforce.console.isInConsole() || isConsole == 'true'){
            OpenSpkrRequestPrimaryTab('{!URLFOR($Action.Activity_Detail__c.Meeting_Preparation_Sheet, callNote.Id)}')
        }
        else if((typeof sforce != 'undefined') && sforce && (!!sforce.one)){
            // Salesforce1 navigation
            sforce.one.navigateToURL('{!URLFOR($Action.Activity_Detail__c.Meeting_Preparation_Sheet, callNote.Id)}');
        }
        else{
            window.open('{!URLFOR($Action.Activity_Detail__c.Meeting_Preparation_Sheet, callNote.Id)}');   
        } 
    }
    
    function openDetailSheet(){
        window.open('{!pageURLForDetailSheet}',"_blank");
    }
    
</script>

