<!---
Visualforce Page Name :  MFS_Activity360PageSF1
Version               :  1.0 
Created Date          :  6 OCT 2016
Function              :  Activity 360 page for Institutional
--->
<apex:page controller="MFS_Activity360Controller" standardStylesheets="false" showHeader="false" sidebar="false" docType="html-5.0">
<html xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no" />
    <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
    <title>{!$Label.A360_MFS_Activity_360_Institutional}</title>
    <link rel="stylesheet" href="/resource/MFS_UI_Assets/assets/styles/salesforce-lightning-design-system-ltng.min.css" />
    <link rel="stylesheet" href="/resource/MFS_UI_Assets/assets/styles/mfs_new_styles.css" />
</head>
<body>
<div class="slds MFS">
<apex:form >
    <span class="slds-hide">
        <apex:actionFunction name="createEventRequest" action="{!createEventRequest}"/>
        <apex:actionFunction name="searchPortfolioOnEnter" action="{!searchProduct}" reRender="searchResults"/>
        <apex:actionFunction name="searchComPortfolioOnEnter" action="{!searchComProduct}" reRender="searchResultscom"/>
        <apex:actionFunction name="searchInvitee" action="{!searchInvitee}" rerender="BoxPanel2" oncomplete="postReRenderActions();"/>
        <apex:actionFunction name="clickRightArrow" action="{!clickRight}" reRender="BoxPanel2" />
        <apex:actionFunction name="clickLeftArrow" action="{!clickLeft}" reRender="BoxPanel2" />
        <apex:actionFunction name="changeHostForInvitees" action="{!changeHostForInvitees}" rerender="eATI"/>
        <apex:actionFunction name="dateTimeChange" action="{!startDateTimeChange}" reRender="endDateTime, eATI" />
        <apex:actionFunction name="changeInviteeTimeSlots" action="{!changeInviteeTimeSlots}" reRender="eATI"/>
        <apex:actionFunction name="changeEventLocation" action="{!changeLocation}" reRender="evLoc" />
        <apex:actionFunction name="removeInvitee" action="{!removeInvitee}" rerender="eATI" oncomplete="initInviteesPortfolioLookup();">
            <apex:param name="inviteeId" value="" assignTo="{!removeInviteeId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="sortInviteesByTime" action="{!sortInviteesByTime}" rerender="eATI">
            <apex:param name="order" value="" assignTo="{!sortingOrder}"/>
        </apex:actionFunction>
        <apex:actionFunction name="sortInviteesByOrg" action="{!sortInviteesByOrg}" rerender="eATI">
            <apex:param name="orderOrg" value="" assignTo="{!sortingOrgOrder}"/>
        </apex:actionFunction>
        <apex:actionFunction name="addPortfToInvitee" action="{!addPortfToInvitee}" rerender="inviteePortf" oncomplete="initInviteesPortfolioLookup();">
            <apex:param name="inviteeId" value="" assignTo="{!inviteeId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="removePortfFromInvitee" action="{!removePortfFromInvitee}" rerender="inviteePortf" oncomplete="initInviteesPortfolioLookup();">
            <apex:param name="removePfRowNum" value="" assignTo="{!removePfRowNum}"/>
            <apex:param id="inviteeId" name="inviteeId" value="" assignTo="{!inviteeId}"/>
        </apex:actionFunction>
       
        <apex:actionFunction name="highInterest" action="{!highInterestProductCheck}" rerender="pbProduct,pbActivity">
            <apex:param name="hI" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="delProduct" action="{!removingProduct}" rerender="pbProduct" immediate="true">
            <apex:param name="prdId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="delCompProduct" action="{!removingCompetitorProduct}" rerender="compProduct" oncomplete="hideLoading();">
            <apex:param name="cmpPrdId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="compAnaysisEdit" action="{!editCompetitiveAnalysisRecord}" oncomplete="postReRenderCARPopUpWindow()" rerender="caPopUpBox">
            <apex:param name="caId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="compAnaysisDelete" action="{!deleteCompAnalysisRecord}" rerender="pbCAR" oncomplete="hideLoading();">
            <apex:param name="caId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="showCompAnaysisDetail" action="{!showCompAnalysisPopup}" oncomplete="postReRenderCARPopUpWindow()" rerender="caPopUpBox"> 
            <apex:param name="caId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="editFollowUpTask" immediate="true" action="{!editFollowUpTaskRecord}" reRender="followUpPopUp,followUpPopUpBtns" oncomplete="postReRenderEditFollowUp();" >
            <apex:param name="tskId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="delFollowUpTask" action="{!deleteFollowUpTaskEvent}" rerender="pbActivity" oncomplete="hideLoading();">
            <apex:param name="tId" value="" assignTo="{!individualChildRecordId}" />
        </apex:actionFunction>
        <apex:actionFunction name="showTaskDetail" action="{!showTaskPopup}" reRender="followUpPopUp,followUpPopUpBtns" oncomplete="postReRenderFollowUpModal();">
            <apex:param name="tId" value="" assignTo="{!individualChildRecordId}" />
        </apex:actionFunction>
        
        <apex:actionFunction name="showMMRDetail" action="{!showMMRPopup}" oncomplete="postReRenderMMRPopUpWindow()" rerender="mmrPopUpBox">
            <apex:param name="mmrId1" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="mmrEdit" action="{!editMMRRecord}" oncomplete="postReRenderMMRPopUpWindow();" rerender="mmrPopUpBox">
            <apex:param name="mmrId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="mmrDelete" action="{!deleteMMRRecord}" oncomplete="hideLoading();" reRender="pbMMR">
            <apex:param name="mmrId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="editFollowUpTask" immediate="true" action="{!editFollowUpTaskRecord}" reRender="followUpPopUp,followUpPopUpBtns" oncomplete="postReRenderEditFollowUp();" >
            <apex:param name="tskId" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        <apex:actionFunction name="delFollowUpTask" action="{!deleteFollowUpTaskEvent}" rerender="pbActivity" oncomplete="hideLoading();">
            <apex:param name="tId" value="" assignTo="{!individualChildRecordId}" />
        </apex:actionFunction>
        <apex:actionFunction name="showTaskDetail" action="{!showTaskPopup}" reRender="followUpPopUp,followUpPopUpBtns" oncomplete="postReRenderFollowUpModal();">
            <apex:param name="tId" value="" assignTo="{!individualChildRecordId}" />
        </apex:actionFunction>
        
        <apex:actionFunction name="spkrRequestDelete" action="{!deleteSpeakerRequestRecord}" oncomplete="hideLoading();" rerender="pbSpkrRequest">
            <apex:param name="srId1" value="" assignTo="{!individualChildRecordId}"/>
        </apex:actionFunction>
        
        <apex:actionFunction name="setSelectedInviteeId" action="{!assignSelectedInviteeId}" rerender="searchResults, addSelectedBtn, searchTextPanel" oncomplete="postReRenderPortfolio();">
            <apex:param name="invId" value="" assignTo="{!invIdForPopup}"/>
            <apex:param name="productSearchBtnName" value="" assignTo="{!productSearchBtnName}"/>
        </apex:actionFunction>        
        
        <apex:actionFunction name="search" action="{!searchProduct}"  reRender="searchResults"/>
        
        <apex:actionFunction name="changeDates" action="{!changeDates}" reRender="res_notfi_dt" />
        <apex:actionFunction name="searchComProduct" action="{!searchComProduct}" reRender="searchResultscom" oncomplete="postReRenderCompPortfolio();" />
        <apex:actionFunction name="resetCompProdSearchPanel" action="{!resetCompProdSearchPanel}" rerender="selectedComProductPanel, searchResultscom" oncomplete="postReRenderCompPortfolio();"/>
    </span>
    
    <div class="slds-page-header" role="banner">
        <div class="slds-grid">
            <div class="slds-col slds-has-flexi-truncate">
                <div class="slds-media slds-no-space slds-grow">
                    <div class="slds-media__figure">
                        <span class="slds-icon_container slds-icon-custom-custom15">
                            <svg aria-hidden="true" class="slds-icon slds-icon--medium">
                                <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/custom-sprite/svg/symbols.svg#custom15"></use>
                            </svg>
                        </span>
                    </div>
                    <div class="slds-media__body">
                        <p class="slds-text-title--caps slds-line-height--reset">{!$Label.A360_Activity_360}</p>
                        <h1 class="slds-page-header__title slds-m-right--small slds-align-middle slds-truncate">{!topPageBlockName}</h1>
                    </div>
                </div>
            </div>
            <div class="slds-col slds-no-flex slds-grid slds-align-top">
                <div class="mfs-top-arrow--small">
                    <button class="slds-button slds-button--icon-border-filled" aria-haspopup="true" title="{!$label.A360_Show_More}">
                        <svg class="slds-button__icon" aria-hidden="true">
                            <use xlink:href="/resource/MFS_UI_Assets/assets/icons/utility-sprite/svg/symbols.svg#down"></use>
                        </svg>
                        <span class="slds-assistive-text">{!$label.A360_Show_More}</span>
                    </button>
                </div>
                <apex:outputPanel id="topBtns" styleClass="slds-button-group mfs-top-buttons-container" html-role="group" layout="block">
                    
                    <apex:outputPanel rendered="{!showResponseBtn == true && disabled==true}">
                        <apex:commandButton value="{!$Label.A360_Accept}" action="{!acceptInvite}" styleclass="slds-button slds-button--neutral slds-truncate" rerender="eATI"/>
                        <apex:commandButton value="{!$Label.A360_Decline}" action="{!declineInvite}" styleclass="slds-button slds-button--neutral slds-truncate" rerender="eATI"/>
                    </apex:outputPanel>
                    
                    <apex:outputPanel rendered="{!!disabled}">
                        <input type="button" value="{!$Label.A360_Save}" class="slds-button slds-button--neutral slds-truncate" onclick="validatePageForm(this, event);" />
                        <apex:commandButton value="{!$Label.A360_Save}" action="{!Save}"  id="saveBtn" onclick="showLoading();" oncomplete="{!IF(showEventRequestPopup,'openSpeakerModal();postReRenderSave();','postReRenderSave();')}" styleClass="slds-button slds-button--neutral slds-truncate" reRender="topBtns,topSectn,pgmsg,IdMeetingMaterialQns,pbProduct,srPopUpBox,pbAttchment,pbSR, mfs_error_panel" style="display:none;" />
                    </apex:outputPanel>
                    
                    <apex:commandButton styleClass="slds-button slds-button--neutral slds-truncate" rendered="{!if(eventCallNote, true,false)}" oncomplete="saveAndClose('{!IF(isNew,parentObjectID,IF(parentEvent.Whatid != NULL,parentEvent.Whatid,parentEvent.Whoid))}', '{!isConsole}');return false;" value="{!$Label.A360_Close}"/>
                    <apex:commandButton styleClass="slds-button slds-button--neutral slds-truncate" rendered="{!if(taskCallNote, true,false)}" oncomplete="saveAndClose('{!IF(isNew,parentObjectID,IF(parentTask.Whatid != NULL,parentTask.Whatid,parentTask.Whoid))}', '{!isConsole}');return false;" value="{!$Label.A360_Close}"/>
                    
                    <apex:commandButton value="{!$Label.A360_Edit}" rendered="{!showResponseBtn == false && disabled==true}" immediate="true" onclick="showLoading();" oncomplete="postReRenderEdit();setAllDayEvent1();ReloadCkEditor();" action="{!edit}" reRender="topBtns,topSectn,pbAttchment" styleClass="slds-button slds-button--neutral slds-truncate"/>
                    
                    <!--<apex:commandButton styleclass="slds-button slds-button--neutral slds-truncate mfs-meeting-prepare-sheet-btn" onclick="openMeetingPrepSheet('{!isConsole}')" id="meetingPrepSheet" value="Meeting Preparation Sheet" rendered="{!showResponseBtn == false && eventCallNote == true}" reRender="null" />-->
                    <apex:commandButton styleclass="slds-button slds-button--neutral slds-truncate mfs_additional_meeting_details" value="{!$Label.A360_Additional_Meeting_Details}" action="{!showMeetingPrepPopup}" oncomplete="postReRenderAdditionalMeetingDetails();" reRender="mpPopUpBox,mpButtonBox" rendered="{!if(eventCallNote, true,false)}"/>
                    <apex:outputPanel rendered="{!if(eventCallNote, true,false)}">
                        <input type="button" class="slds-button slds-button--neutral slds-truncate" value="{!$Label.A360_Meeting_Detail_Sheet}" onclick="openDetailSheet();" />
                    </apex:outputPanel>
                </apex:outputPanel>
            </div>
        </div>
    </div>
    <div class="slds-container--x-large slds-container--center slds-inner-container mfs-main-container" id="scroller">
        <apex:outputPanel layout="block" id="mfs_error_panel">
            <apex:pageMessages ></apex:pageMessages>
        </apex:outputPanel>
        <div class="slds-p-top--small slds-form-element__group slds-grid slds-wrap slds-pull--padded chatter-feeds-container">
            <div class="slds-p-horizontal--small slds-size--12-of-12">
                <div class="slds-clearfix">
                    <div class="slds-float--left slds-m-top--small"><h3 class="slds-section__title slds-text-title--caps">{!$Label.A360_Chatter}</h3></div>
                    <div class="slds-float--left slds-m-left--small mfs-heading-icon">
                        <input type="button" value="{!$Label.A360_Show_Chatter}" class="slds-button slds-button--neutral mfs_hide_chatter" onclick="prevDefault(event);" />
                        <span class="slds-hide show_chatter_label" style="display:none;">{!$Label.A360_Show_Chatter}</span>
                        <span class="slds-hide hide_chatter_label" style="display:none;">{!$Label.A360_Hide_Chatter}</span>
                    </div>
                </div>
            </div>
            
            <div class="slds-p-horizontal--small slds-m-top--medium slds-size--12-of-12 mfs-chatter-feeds" style="display:none;">
                <div>
                    <apex:iframe id="chatter-iframe" src="{!pageURL}" width="100%" height="200px" scrolling="true" />
                </div>
            </div>
        </div>
        
        <apex:outputPanel id="topSectn" layout="block">
            <!-- Task Details (Log a Call) -->
            <apex:outputPanel layout="block" rendered="{!taskCallNote}" styleClass="mfs_task_section slds-form--compound">
                <div class="slds-form-element__group slds-grid slds-wrap slds-pull--padded">
                    <div class="slds-p-horizontal--small slds-size--12-of-12 slds-m-top--large">
                        <div class="slds-clearfix">
                            <div class="slds-float--left slds-m-top--small"><h3 class="slds-section__title slds-text-title--caps">{!$Label.A360_Call_Log_Details}</h3></div>
                        </div>
                    </div>
                    <div class="slds-p-horizontal--small slds-m-top--medium slds-large-size--6-of-12 slds-medium-size--6-of-12 slds-size--12-of-12">
                        <div class="slds-form-element">
                            <label class="slds-form-element__label">{!$Label.A360_Name}</label>
                            <div class="slds-form-element__control">
                                <span class="curr_acc_name">
                                    <apex:outputField rendered="{!disabled}" styleClass="slds-output" value="{!parentTask.WhoId}" />
                                </span>
                                
                                <apex:outputField rendered="{!IF( !disabled && ( AND(isNew,parentObjectType == 'Contact') || AND(NOT(isNew),NOT(ISBLANK(parentTask.WhoId)) ) ),TRUE,FALSE)}" styleClass="slds-output" value="{!parentTask.WhoId}" />
                                <apex:outputPanel rendered="{!IF( !disabled && (AND(isNew,parentObjectType!= 'Contact') || AND(NOT(isNew),ISBLANK(parentTask.WhoId))) ,TRUE,FALSE)}" layout="block">
                                    <div class="slds-form-element slds-lookup slds-lookup--polymorphic" data-select="single" data-callback="MFSApp.selectEventName">
                                        <div class="slds-form-element__control slds-grid slds-box--border">
                                            <div class="dropdown--trigger slds-dropdown-trigger--click slds-align-middle slds-m-left--xx-small slds-shrink-none">
                                                <span class="slds-icon__container selected-icon-container">
                                                    <svg aria-hidden="true" class="slds-icon slds-icon-standard-opportunity slds-icon--small slds-m-right--small">
                                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/standard-sprite/svg/symbols.svg#contact"></use>
                                                    </svg>
                                                </span>
                                                
                                                <button class="slds-button slds-button--icon slds-button--icon-small slds-hide" aria-haspopup="true" data-query-default-url='MFS_Activity360Controller.queryContacts'>
                                                    <svg aria-hidden="true" class="slds-button__icon">
                                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/utility-sprite/svg/symbols.svg#down"></use>
                                                    </svg>
                                                    <span class="slds-assistive-text">{!$Label.A360_Filter_By_object}</span>
                                                </button>
                                                
                                                <div class="slds-dropdown slds-dropdown--left">
                                                    <ul class="dropdown__list" role="menu">
                                                        <li class="slds-dropdown__item" role="presentation" data-query-url='MFS_Activity360Controller.queryContacts'>
                                                            <a href="javascript:void(0);" role="menuitem" tabindex="-1">
                                                                <span class="slds-icon__container">
                                                                    <span>
                                                                        <svg aria-hidden="true" class="slds-icon slds-icon--small slds-icon-standard-opportunity slds-m-right--small">
                                                                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/standard-sprite/svg/symbols.svg#contact"></use>
                                                                        </svg>
                                                                    </span>
                                                                    {!$Label.A360_Contact}
                                                                </span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                            
                                            <div class="slds-input-has-icon slds-input-has-icon--right slds-grow">
                                                <svg aria-hidden="true" class="slds-input__icon">
                                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/utility-sprite/svg/symbols.svg#search"></use>
                                                </svg>
                                                
                                                <apex:inputHidden value="{!parentTask.WhoId}" />
                                                <input class="slds-lookup__search-input slds-input--bare" value="{!whoName}" type="text" placeholder="{!$Label.A360_Search_contact}" aria-activedescendent="" aria-expanded="false" aria-autocomplete="list" />
                                            </div>
                                        </div>
                                        
                                        <div class="slds-lookup__menu slds-hide">
                                            <ul class="slds-lookup__list" role="listbox"></ul>
                                        </div>
                                    </div>
                                </apex:outputPanel>
                            </div>
                        </div>
                    </div>
                    <div class="slds-p-horizontal--small slds-m-top--medium slds-large-size--6-of-12 slds-medium-size--6-of-12 slds-size--12-of-12">
                   
                        <apex:outputPanel rendered="{!disabled}" layout="block" styleClass="slds-grid slds-wrap">
                            <div class="slds-size--6-of-12 slds-p-horizontal--small slds-p-left--none">
                                <div class="slds-form--stacked">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">{!$Label.A360_Date_Time_Called}</label>
                                        <div class="slds-form-element__control">
                                            <div class="slds-input-has-icon slds-input-has-icon--right">
                                                <apex:outputField value="{!parentTask.ActivityDate}"/>&nbsp;<apex:outputText value="{!parentTask.Called_Time__c}"/>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </apex:outputPanel>
                        
                        <apex:outputPanel layout="block" rendered="{!!disabled}" styleClass="slds-grid slds-wrap">
                            <div class="slds-size--6-of-12 slds-p-horizontal--small slds-p-left--none">
                                <div class="slds-form--stacked">
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label">{!$Label.A360_Date_Time_Called}</label>
                                        <div class="slds-form-element__control">
                                            <div class="slds-input-has-icon slds-input-has-icon--right">
                                                <svg aria-hidden="true" class="slds-input__icon slds-icon-text-default">
                                                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/utility-sprite/svg/symbols.svg#event"></use>
                                                </svg>
                                                
                                                <apex:inputField value="{!parentTask.ActivityDate}" styleClass="slds-input" html-data-callback="setPageModifiedEvent" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="slds-size--6-of-12 slds-p-horizontal--small slds-p-right--none">
                                <div class="slds-form-element mfs_event_time_field">
                                    <label class="slds-form-element__label">&nbsp;</label>
                                    <div class="slds-select_container">
                                        <apex:inputField value="{!parentTask.Called_Time__c}"  styleClass="slds-select"/>
                                    </div>
                                </div>
                            </div>
                        </apex:outputPanel>
                    </div>
                    <div class="slds-p-horizontal--small slds-m-top--medium slds-large-size--6-of-12 slds-medium-size--6-of-12 slds-size--12-of-12">
                        <div class="slds-form-element">
                            <label class="slds-form-element__label">{!$Label.A360_Related_To}</label>
                            <div class="slds-form-element__control">
                                <span class="curr_related_to">
                                    <apex:outputField rendered="{!disabled}" id="task_what_id_disabled" styleClass="slds-output" value="{!parentTask.WhatId}" />
                                </span>
                                
                                <span class="slds-hide what_id_object_name_new">{!whatIdObjectName}</span>
                                <span class="slds-hide what_name_new">{!whatName}</span>
                                
                                <apex:outputPanel id="task_what_id" rendered="{!!disabled}">
                                    <div class="slds-form-element slds-lookup slds-lookup--polymorphic" data-select="single" data-callback="MFSApp.selectEventRelatedTo">
                                        <div class="slds-form-element__control slds-grid slds-box--border">
                                            <div class="dropdown--trigger slds-dropdown-trigger--click slds-align-middle slds-m-left--xx-small slds-shrink-none">
                                                <span class="slds-icon__container selected-icon-container what-handle-icon-container">
                                                    <svg aria-hidden="true" class="slds-icon slds-icon-standard-account slds-icon--small slds-m-right--small">
                                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/standard-sprite/svg/symbols.svg#account"></use>
                                                    </svg>
                                                </span>
                                                <button class="slds-button slds-button--icon slds-button--icon-small" aria-haspopup="true" data-query-default-url="{!remoteActionQueryURL}">
                                                    <svg aria-hidden="true" class="slds-button__icon">
                                                        <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/resource/MFS_UI_Assets/assets/icons/utility-sprite/svg/symbols.svg#down"></use>
                                                    </svg>
                                                    <span class="slds-assistive-text">{!$Label.A360_Filter_By_object}</span>
                                                </button>
                                                
                                                <div class="slds-dropdown slds-dropdown--left">
                                                    <ul class="dropdown__list" role="menu">
